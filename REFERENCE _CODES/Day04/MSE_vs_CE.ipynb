   "source": [
    "def MSE(y, y_hat):\n",
    "    return ((y - y_hat) ** 2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "e1d04ea1-2a15-4124-9a7e-066c4abc2f3d",
   "metadata": {},
   "outputs": [],
   "source": [
    "def CE(y, y_hat):\n",
    "    return -((y * np.log(y_hat)) + ((1 - y) * np.log(1 - y_hat)))"
   ]
  },
